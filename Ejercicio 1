int main() {
	int a;
	int i;
	int n;
	int vec[ARREGLO_MAX];
	cout << "Ingrese el tamaÃ±o del Vector" << endl;
	cin >> n;
	a = 0;
	i = 1;
	while (i<=n) {
		cout << "El valor actual de I:" << i << endl;
		cout << "Ingrese un valor para A" << endl;
		cin >> a;
		vec[i-1] = a;
		i = i+1;
	}
	i = 1;
	while (i<=n) {
		cout << vec[i-1] << "-";
		i = i+1;
	}
	return 0;
}

